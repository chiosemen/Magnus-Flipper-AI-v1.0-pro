{
  "cli": {
    "version": ">=5.0.0"
  },
  "build": {
    "production": {
      "android": {
        "buildType": "app-bundle",
        "package": "com.magnusflipper.ai"
      },
      "ios": {
        "simulator": false,
        "bundleIdentifier": "com.magnusflipper.ai"
      },
      "env": {
        "APP_ENV": "production"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "package": "com.magnusflipper.ai"
      },
      "ios": {
        "simulator": true,
        "bundleIdentifier": "com.magnusflipper.ai"
      },
      "env": {
        "APP_ENV": "preview"
      }
    },
    "development": {
      "android": {
        "buildType": "apk",
        "package": "com.magnusflipper.ai"
      },
      "ios": {
        "simulator": true,
        "bundleIdentifier": "com.magnusflipper.ai"
      },
      "env": {
        "APP_ENV": "development"
      },
      "developmentClient": true
    }
  }
}
