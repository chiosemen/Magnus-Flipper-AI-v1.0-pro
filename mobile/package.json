    "build:all": "eas build --platform all --profile production",
    "build:preview": "eas build --platform all --profile preview",
    "submit:android": "eas submit -p android",
    "submit:ios": "eas submit -p ios",
    "lint": "expo lint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
