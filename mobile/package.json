    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.17.9",
    "axios": "^1.6.5",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "expo": "^52.0.0",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.20",
    "expo-device": "~7.0.3",
