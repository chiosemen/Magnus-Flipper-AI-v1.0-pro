# Magnus Flipper Web App - Production Environment Variables

# API Configuration
NEXT_PUBLIC_API_URL=https://api.magnus-flipper.com
NEXT_PUBLIC_SOCKET_URL=wss://api.magnus-flipper.com

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# App Configuration
NEXT_PUBLIC_APP_NAME=Magnus Flipper
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_ENV=production

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_ERROR_TRACKING=true

# Asset CDN (if using)
NEXT_PUBLIC_ASSET_CDN=https://cdn.magnus-flipper.com
